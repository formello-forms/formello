    "title": "Room booking",
    "content": "<!-- wp:formello/form {\"lock\":{\"move\":false,\"remove\":true}} -->\n<form method=\"post\" class=\"wp-block-formello-form\" novalidate><input type=\"hidden\" name=\"_formello_id\"/><input type=\"text\" class=\"formello-hp\" tabindex=\"-1\"/><input type=\"hidden\" name=\"action\" value=\"formello\"/><!-- wp:group {\"layout\":{\"type\":\"flex\",\"flexWrap\":\"nowrap\"}} -->\n<div class=\"wp-block-group\"><!-- wp:formello/input {\"style\":{\"layout\":{\"selfStretch\":\"fill\",\"flexSize\":null}}} -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>First Name</span><span class=\"required\" aria-hidden=\"true\">*</span></label><input name=\"name\" type=\"text\" required class=\"\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input -->\n\n<!-- wp:formello/input {\"style\":{\"layout\":{\"selfStretch\":\"fill\",\"flexSize\":null}}} -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Last Name</span><span class=\"required\" aria-hidden=\"true\">*</span></label><input name=\"lastname\" type=\"email\" required class=\"\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input --></div>\n<!-- /wp:group -->\n\n<!-- wp:formello/input -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Email</span></label><input name=\"email\" type=\"email\" class=\"\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input -->\n\n<!-- wp:formello/input {\"flatpickr\":{\"noCalendar\":false,\"minDate\":\"today\",\"mode\":\"range\"},\"advanced\":true} -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Date</span></label><input name=\"date\" type=\"date\" data-flatpickr=\"{&quot;noCalendar&quot;:false,&quot;minDate&quot;:&quot;today&quot;,&quot;mode&quot;:&quot;range&quot;}\" class=\"forme