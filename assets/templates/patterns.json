    "title": "Barber Shop Booking",
    "content": "<!-- wp:formello/form {\"lock\":{\"move\":false,\"remove\":true}} -->\n<form method=\"post\" class=\"wp-block-formello-form\" novalidate><input type=\"hidden\" name=\"_formello_id\"/><input type=\"text\" class=\"formello-hp\" tabindex=\"-1\"/><input type=\"hidden\" name=\"action\" value=\"formello\"/><!-- wp:formello/input {\"advanced\":false} -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Name</span><span class=\"required\" aria-hidden=\"true\">*</span></label><input name=\"name\" type=\"text\" required class=\"\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input -->\n\n<!-- wp:formello/input -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Email</span><span class=\"required\" aria-hidden=\"true\">*</span></label><input name=\"email\" type=\"email\" required class=\"\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input -->\n\n<!-- wp:formello/input {\"advanced\":true} -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Phone</span><span class=\"required\" aria-hidden=\"true\">*</span></label><input name=\"phone\" type=\"tel\" required class=\"formello-advanced\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input -->\n\n<!-- wp:formello/fieldset -->\n<fieldset class=\"wp-block-formello-fieldset\"><legend>Select the treatments you'd like to book</legend><!-- wp:formello/multichoices {\"required\":true} -->\n<div class=\"wp-block-formello-multichoices\"><div class=\"wp-block-formello-input\"><label for=\"my_choice-0\">Cut and style</label><input value=\"1\" name=\"my_choice[]\" type=\"checkbox\" required id=\"my_choice-0\" data-wp-on--input=\"actions.validateInput\" data-wp-on--blur=\"actions.validateInput\" data-wp-on--invalid=\"actions.validateInput\"/></div><div class=\"wp-block-formello-input\"><label for=\"my_choice-1\">Beard trim</label><input value=\"2\" name=\"my_choice[]\" type=\"checkbox\" required id=\"my_choice-1\" data-wp-on--input=\"actions.validateInput\" data-wp-on--blur=\"actions.validateInput\" data-wp-on--invalid=\"actions.validateInput\"/></div><div class=\"wp-block-formello-input\"><label for=\"my_choice-2\">Manicure</label><input value=\"3\" name=\"my_choice[]\" type=\"checkbox\" required id=\"my_choice-2\" data-wp-on--input=\"actions.validateInput\" data-wp-on--blur=\"actions.validateInput\" data-wp-on--invalid=\"actions.validateInput\"/></div><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/multichoices --></fieldset>\n<!-- /wp:formello/fieldset -->\n\n<!-- wp:formello/input {\"advanced\":true} -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Date</span><span class=\"required\" aria-hidden=\"true\">*</span></label><input name=\"date\" type=\"date\" required data-flatpickr=\"[]\" class=\"formello-advanced\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input -->\n\n<!-- wp:formello/input {\"advanced\":false} -->\n<div class=\"wp-block-formello-input\" data-wp-context=\"true\"><label for=\"id\"><span>Time</span><span class=\"required\" aria-hidden=\"true\">*</span></label><input name=\"time\" type=\"time\" required step=\"300\" min=\"09:00\" max=\"20:00\" class=\"\"/><div class=\"error-message\" data-wp-text=\"context.error\"></div></div>\n<!-- /wp:formello/input -->\n\n<!-- wp:formello/button -->\n<div class=\"wp-block-formello-button left\"><button type=\"submit\" class=\"wp-element-button has-text-color has-background\" style=\"color:#ffffff;background-color:#000000\"><span>Submit</span><svg xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 40 40\"><path d=\"M20.201 5.169c-8.254 0-14.946 6.692-14.946 14.946 0 8.255 6.692 14.946 14.946 14.946s14.946-6.691 14.946-14.946c-.001-8.254-6.692