{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": "2",
	"name": "formello/form",
	"title": "Form",
	"description": "A block collection to build forms.",
	"category": "formello",
	"textdomain": "formello",
	"keywords": ["form", "forms", "formello"],
	"attributes": {
		"id": {
			"type": "number"
		},
		"name": {
			"type": "string",
			"source": "attribute",
			"selector": "form",
			"attribute": "name"
		},
		"successMessage": {
			"type": "string",
			"default": ""
		},
		"errorMessage": {
			"type": "string",
			"default": ""
		},
		"redirectUrl": {
			"type": "string",
			"source": "attribute",
			"selector": "form",
			"attribute": "data-redirect",
			"default": ""
		},
		"debug": {
			"type": "boolean",
			"default": false
		},
		"recaptchaEnabled": {
			"type": "boolean",
			"source": "attribute",
			"selector": "form",
			"attribute": "data-recaptcha",
			"default": false
		},
		"recaptchaVersion": {
			"type": "number",
			"default": 3
		},
		"labelIsBold": {
			"type": "boolean",
			"default": false
		},
		"labelAlign": {
			"type": "string",
			"default": ""
		},
		"requiredText": {
			"type": "string",
			"default": "*"
		},
		"storeSubmissions": {
			"type": "boolean",
			"default": true
		},
		"hide": {
			"type": "boolean",
			"source": "attribute",
			"selector": "form",
			"attribute": "data-hide",
			"default": false
		},
		"asRow": {
			"type": "boolean",
			"default": false
		},
		"autoComplete": {
			"type": "string",
			"source": "attribute",
			"selector": "form",
			"attribute": "autocomplete",
			"default": "off"
		},
		"actions": {
			"type": "array",
			"default": []
		}		
	},
	"styles": [
		{
			"name": "regular",
			"label": "Default",
			"isDefault": true
		},
		{ "name": "bolded", "label": "Bolded label" }
	],
	"editorScript": "file:../../../build/index.js",
	"editorStyle": "file:../../../build/index.css",
	"style": "file:../../../build/style-index.css",
	"viewScript": "file:../../../build/frontend.js"
}