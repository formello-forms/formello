{
	"globals": {
		"wp": true,
		"wpApiSettings": true,
		"window": true,
		"document": true,
		"pagenow": true,
		"grecaptcha": true,
		"tinymce": true,
		"flatpickr": true,
		"popper": true,
		"formello": true
	},
	"env": {
	  "browser": true,
	  "node": true,
	  "es6": true,
	  "jest/globals": true
	},
	"parserOptions": {
	  "ecmaVersion": 6,
	  "sourceType": "module",
	  "ecmaFeatures": {
		"jsx": true,
		"experimentalObjectRestSpread": true
	  }
	},
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended-with-formatting"
	],
	"plugins": ["react", "jsx-a11y", "jest"],
	"settings": {
		"react": {
			"pragma": "wp"
		}
	},
	"rules": {
		"no-shadow": "off"
	}
}